{
  "name": "hotel-extranet-frontend",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "build:production": "NODE_ENV=production vite build",
    "build:staging": "NODE_ENV=staging vite build",
    "build:development": "NODE_ENV=development vite build",
    "build:preview": "NODE_ENV=preview vite build",
    "preview": "vite preview",
    "preview:production": "vite preview --mode production",
    "preview:staging": "vite preview --mode staging",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "lint:fix": "eslint . --ext ts,tsx --fix",
    "type-check": "tsc --noEmit",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest --coverage",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "prebuild": "npm run lint && npm run type-check && npm run test",
    "postbuild": "npm run test:e2e",
    "deploy:netlify": "netlify deploy --prod --dir=dist",
    "deploy:vercel": "vercel --prod",
    "deploy:github": "npm run build && gh-pages -d dist",
    "deploy:docker": "docker build -t hotel-extranet-frontend . && docker run -p 3000:80 hotel-extranet-frontend",
    "analyze": "vite-bundle-analyzer",
    "clean": "rm -rf dist node_modules/.vite",
    "install:clean": "rm -rf node_modules package-lock.json && npm install"
  },
  "dependencies": {
    "@coreui/coreui": "^5.0.0",
    "@coreui/icons": "^3.0.0",
    "@coreui/react": "^5.0.0",
    "@hookform/resolvers": "^3.3.2",
    "@tanstack/react-query": "^5.8.4",
    "@types/react": "^18.2.37",
    "@types/react-dom": "^18.2.15",
    "axios": "^1.6.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.47.0",
    "react-hot-toast": "^2.4.1",
    "react-router-dom": "^6.18.0",
    "web-vitals": "^3.5.0"
  },
  "devDependencies": {
    "@playwright/test": "^1.39.0",
    "@types/node": "^20.8.10",
    "@typescript-eslint/eslint-plugin": "^6.10.0",
    "@typescript-eslint/parser": "^6.10.0",
    "@vitejs/plugin-react": "^4.1.0",
    "@vitest/coverage-v8": "^0.34.6",
    "autoprefixer": "^10.4.16",
    "eslint": "^8.53.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.4",
    "gh-pages": "^6.0.0",
    "netlify-cli": "^17.10.1",
    "postcss": "^8.4.31",
    "tailwindcss": "^3.3.5",
    "typescript": "^5.2.2",
    "vercel": "^32.7.0",
    "vite": "^4.5.0",
    "vite-bundle-analyzer": "^0.7.0",
    "vitest": "^0.34.6"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/your-username/hotel-extranet.git"
  },
  "keywords": [
    "hotel",
    "extranet",
    "booking",
    "management",
    "react",
    "typescript",
    "vite"
  ],
  "author": "Your Name <your.email@example.com>",
  "license": "MIT",
  "homepage": "https://your-username.github.io/hotel-extranet"
}